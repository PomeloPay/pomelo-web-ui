{
  "name": "pomelopay-web-ui",
  "version": "0.0.79",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/custom-elements/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/custom-elements/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/pomelopay-web-ui/pomelopay-web-ui.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:css": "cp www/build/pomelopay-webui.css dist/pomelopay-webui.css",
    "build:config": "cp src/config/theme.js dist/theme.js",
    "build": "stencil build --docs && yarn build:css && yarn build:config",
    "start": "stencil build --dev --watch --serve --no-open",
    "dev:js": "stencil build --dev --watch",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -s www --ci && yarn start:js",
    "build-storybook": "stencil build --prod && build-storybook",
    "build-www": "concurrently \"NODE_ENV=prod stencil build --prod\" \"build-storybook -o public\""
  },
  "dependencies": {
    "@popperjs/core": "^2.5.2",
    "@stencil/core": "^2.3.0",
    "animejs": "^3.2.0",
    "cleave.js": "^1.6.0",
    "clsx": "^1.1.1",
    "intl-tel-input": "^17.0.8",
    "nanoid": "^3.1.20",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^2.0.4"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@stencil/postcss": "^2.0.0",
    "@storybook/addon-actions": "^6.2.1",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-storysource": "^6.2.1",
    "@storybook/addon-viewport": "^6.2.1",
    "@storybook/html": "^6.2.1",
    "@types/autoprefixer": "^10.2.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.14.37",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cssnano": "^4.1.10",
    "postcss": "^8.2.9",
    "postcss-cli": "^8.3.1",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^14.0.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-replace": "^1.1.3",
    "purge-from-html": "^1.0.3",
    "purgecss-from-html": "^4.0.3",
    "purgecss-from-js": "^1.0.0",
    "react-is": "^17.0.2",
    "serverless-finch": "^2.6.0",
    "storybook-addon-playroom": "^2.0.0",
    "stylelint-config-recommended": "^4.0.0"
  }
}
